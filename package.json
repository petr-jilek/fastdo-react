{
  "name": "fastdo-react",
  "version": "0.0.6",
  "description": "Component library",
  "author": "Petr JÃ­lek",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "typescript",
    "ui"
  ],
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENCE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petr-jilek/fastdo-react.git"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "postbuild": "copyfiles -u 1 \"./src/**/*.css\" \"dist/esm\""
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.6",
    "axios": "^1.4.0",
    "react-icons": "^4.10.1",
    "react-select": "^5.7.3"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "copyfiles": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  }
}
